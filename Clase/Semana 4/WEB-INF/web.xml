<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>com.mycompany.sesion04.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistroPacienteServlet</servlet-name>
        <servlet-class>com.hospital.servlet.RegistroPacienteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/presentacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistroPacienteServlet</servlet-name>
        <url-pattern>/registroPaciente</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
